#include <stdio.h>
#include "main.h"
/**
*jack_bauer - prints every minute of the day of Jack Bauer
*
* Return: Always 0
*/
void jack_bauer(void)
{
int h;
 while (h < 24)
{
int m;
while (m < 60)
{
_putchar((h / 10) + '0');
_putchar((h % 10) + '0');
_putchar(':');
_putchar((m / 10) + '0');
_putchar((m % 10) + '0');
_putchar('\n');
m++;
}
h++;
}
}
